<Window x:Class="POE_Part_3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POE_Part_3"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
        <TabControl>
            <TabItem Header="Income and expenditure">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="45*"/>
                        <ColumnDefinition Width="129*"/>
                        <ColumnDefinition Width="820*"/>
                    </Grid.ColumnDefinitions>
                    <Rectangle HorizontalAlignment="Left" Height="436" Margin="119,68,0,0" Stroke="Black" VerticalAlignment="Top" Width="660" Grid.ColumnSpan="2" Grid.Column="1"/>
                    <Label Content="Income: " HorizontalAlignment="Left" Margin="53,137,0,0" VerticalAlignment="Top" Grid.Column="2" FontSize="17"/>
                    <Label Content="Groceries: " HorizontalAlignment="Left" Margin="53,202,0,0" VerticalAlignment="Top" Grid.Column="2" FontSize="17"/>
                    <Label Content="Utilities: " HorizontalAlignment="Left" Margin="53,262,0,0" VerticalAlignment="Top" Grid.Column="2" FontSize="17"/>
                    <Label Content="Travel cost: " HorizontalAlignment="Left" Margin="53,315,0,0" VerticalAlignment="Top" Grid.Column="2" FontSize="17"/>
                    <Label Content="Cellphone: " HorizontalAlignment="Left" Margin="53,380,0,0" VerticalAlignment="Top" Grid.Column="2" FontSize="17"/>
                    <Label Content="Other expenses: " HorizontalAlignment="Left" Margin="53,445,0,0" VerticalAlignment="Top" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtIncome" HorizontalAlignment="Left" Margin="263,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtGroceries" HorizontalAlignment="Left" Margin="263,210,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtWaterLights" HorizontalAlignment="Left" Margin="263,262,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtTravel" HorizontalAlignment="Left" Margin="263,323,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtPhone" HorizontalAlignment="Left" Margin="263,388,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtExpenses" HorizontalAlignment="Left" Margin="263,453,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <Label Content="Income and expenses" Grid.Column="2" HorizontalAlignment="Left" Margin="199,10,0,0" VerticalAlignment="Top" FontSize="25"/>
                    <Label Content="Please enter the relevant information" Grid.Column="2" HorizontalAlignment="Left" Margin="160,90,0,0" VerticalAlignment="Top" FontSize="17"/>
                </Grid>

            </TabItem>
            <TabItem Header="Accomodation">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="14*"/>
                        <ColumnDefinition Width="68*"/>
                        <ColumnDefinition Width="29*"/>
                        <ColumnDefinition Width="135*"/>
                        <ColumnDefinition Width="748*"/>
                    </Grid.ColumnDefinitions>
                    <Rectangle HorizontalAlignment="Left" Height="511" Stroke="Black" VerticalAlignment="Center" Width="960" Grid.Column="1" Grid.ColumnSpan="4" Margin="10,0,0,0"/>
                    <Label Content="Rent: " Grid.Column="3" HorizontalAlignment="Left" Margin="10,286,0,0" VerticalAlignment="Top" FontSize="17" Width="57"/>
                    <Label Content="Number of months: " Grid.Column="4" HorizontalAlignment="Left" Margin="307,447,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="Interest: " Grid.Column="4" HorizontalAlignment="Left" Margin="299,371,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="Deposit: " Grid.Column="4" HorizontalAlignment="Left" Margin="298,286,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="House amount: " Grid.Column="4" HorizontalAlignment="Left" Margin="298,205,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <TextBox x:Name="txtNumOfMonths" Grid.Column="4" HorizontalAlignment="Left" Margin="508,451,0,0" Text="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" FontSize="17"/>
                    <TextBox x:Name="txtHouseInterest" Grid.Column="4" HorizontalAlignment="Left" Margin="508,371,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" FontSize="17"/>
                    <TextBox x:Name="txtDeposit" Grid.Column="4" HorizontalAlignment="Left" Margin="508,290,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" FontSize="17"/>
                    <TextBox x:Name="txtPurchasePrice" Grid.Column="4" HorizontalAlignment="Left" Margin="508,213,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" FontSize="17"/>
                    <TextBox x:Name="txtMonthRent" Grid.Column="4" HorizontalAlignment="Left" Margin="10,290,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="121" FontSize="17"/>
                    <Rectangle Grid.Column="4" HorizontalAlignment="Left" Height="401" Margin="239,112,0,0" Stroke="Black" VerticalAlignment="Top" Width="404"/>
                    <Rectangle Grid.Column="1" HorizontalAlignment="Left" Height="401" Margin="34,112,0,0" Stroke="Black" VerticalAlignment="Top" Width="404" Grid.ColumnSpan="4"/>
                    <Label Content="Please fill in the relevant information for house purchasing." Grid.Column="4" HorizontalAlignment="Left" Margin="239,155,0,0" VerticalAlignment="Top" FontSize="15" Height="45" Width="404"/>
                    <Label Content="Please fill in the relevant information for renting." Grid.Column="2" HorizontalAlignment="Left" Margin="0,156,0,0" VerticalAlignment="Top" FontSize="15" Grid.ColumnSpan="3"/>
                    <Label Content="Renting" Grid.Column="3" HorizontalAlignment="Left" Margin="97,119,0,0" VerticalAlignment="Top" FontSize="20" Grid.ColumnSpan="2"/>
                    <Label Content="Housing" Grid.Column="4" HorizontalAlignment="Left" Margin="399,118,0,0" VerticalAlignment="Top" FontSize="20"/>
                    <Label Content="Fill in the information for either renting or purchasing a house." Grid.Column="3" HorizontalAlignment="Left" Margin="126,41,0,0" VerticalAlignment="Top" FontSize="17" Grid.ColumnSpan="2"/>
                </Grid>
            </TabItem>
            <TabItem Header="Vehicle">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="303*"/>
                        <ColumnDefinition Width="12*"/>
                        <ColumnDefinition Width="679*"/>
                    </Grid.ColumnDefinitions>
                    <Rectangle HorizontalAlignment="Left" Height="436" Margin="119,68,0,0" Stroke="Black" VerticalAlignment="Top" Width="660" Grid.ColumnSpan="3"/>
                    <Label Content="Car Insurance: " HorizontalAlignment="Left" Margin="200,400,0,0" VerticalAlignment="Top" FontSize="17" Grid.ColumnSpan="2" Width="115"/>
                    <Label Content="Car Interest: " HorizontalAlignment="Left" Margin="200,355,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="Car Deposit: " HorizontalAlignment="Left" Margin="200,308,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="Car Price: " HorizontalAlignment="Left" Margin="200,260,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="Car Type: " HorizontalAlignment="Left" Margin="200,209,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <TextBox x:Name="txtCarinsurance" HorizontalAlignment="Left" Margin="74,408,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtCarinterest" HorizontalAlignment="Left" Margin="74,363,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtCardeposit" HorizontalAlignment="Left" Margin="74,316,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtCarPrice" HorizontalAlignment="Left" Margin="74,268,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <TextBox x:Name="txtMake" HorizontalAlignment="Left" Margin="74,217,0,0" Text="None" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="2" FontSize="17"/>
                    <Label Content="Vehicle Purchase" Grid.Column="2" HorizontalAlignment="Left" Margin="56,84,0,0" VerticalAlignment="Top" FontSize="20"/>
                    <Label Content="If you would like to purchase a vehicle, please enter relevant information." HorizontalAlignment="Left" Margin="173,144,0,0" VerticalAlignment="Top" FontSize="17" Grid.ColumnSpan="3"/>
                </Grid>
            </TabItem>
            <TabItem Header="Savings" FontSize="12">
                <Grid Background="#FFE5E5E5">
                    <Rectangle HorizontalAlignment="Left" Height="436" Margin="119,68,0,0" Stroke="Black" VerticalAlignment="Top" Width="660" Grid.ColumnSpan="2" Grid.Column="1"/>
                    <Label Content="Interest Rate: " HorizontalAlignment="Left" Margin="252,405,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="Years: " HorizontalAlignment="Left" Margin="252,336,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="Savings: " HorizontalAlignment="Left" Margin="252,260,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <TextBox x:Name="txtSaveInterestRate" HorizontalAlignment="Left" Margin="449,413,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="17"/>
                    <TextBox x:Name="txtYears" HorizontalAlignment="Left" Margin="449,344,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="17"/>
                    <TextBox x:Name="txtSavings" HorizontalAlignment="Left" Margin="449,268,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="17"/>
                    <Label Content="Savings" HorizontalAlignment="Left" Margin="411,100,0,0" VerticalAlignment="Top" FontSize="20"/>
                    <Label Content="Fill in the relevant details of how much you would like to save, and we will calculate " HorizontalAlignment="Left" Margin="129,158,0,0" VerticalAlignment="Top" FontSize="17"/>
                    <Label Content="the monthly money needed to reach that goal." HorizontalAlignment="Left" Margin="270,196,0,0" VerticalAlignment="Top" FontSize="17"/>
                </Grid>
            </TabItem>

        </TabControl>



    </Grid>
</Window>
